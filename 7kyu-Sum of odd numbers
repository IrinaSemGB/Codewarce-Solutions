JavaScript

function rowSumOddNumbers(n) {
  let num = 1;
  let arr = [];
  for (let i = 1; i <= n; i++) {
    let sum = 0;
    for (let j = 0; j < i; j++) {
      sum += num;
      num += 2;
    }
    arr.push(sum);
  }
  return arr[n-1];
}

Swift

func rowSumOddNumbers(_ row: Int) -> Int {
  return Int(pow(Double(row), 3))
}
