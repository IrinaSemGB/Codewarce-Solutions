JavaScript solution

function switchItUp(number){
  const arr = ['Zero', 'One', 'Two', 'Three', 'Four', 'Five', 'Six', 'Seven', 'Eight', 'Nine'];
  return arr[number];
}

Swift solution

func switchItUp(_ number: Int) -> String {
  var result: String = ""
  switch number {
    case 0:
      result = "Zero"
    case 1:
      result = "One"
    case 2:
      result = "Two"
    case 3:
      result = "Three"
    case 4:
      result = "Four"
    case 5:
      result = "Five"
    case 6:
      result = "Six"
    case 7:
      result = "Seven"
    case 8:
      result = "Eight"
    case 9:
      result = "Nine"
    default:
      result = ""
  }
  return result
}
